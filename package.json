  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "loadable-components": "^2.2.3",
    "react": "^16.6.0",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-dom": "^16.6.0",
    "react-feather": "^1.1.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1"
  },
  "devDependencies": {
    "babel-loader": "^8.0.4",
    "mdx-loader": "^2.0.1",
    "push-dir": "^0.4.1",
    "react-snap": "^1.22.0"
  }
