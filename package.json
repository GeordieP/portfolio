{
  "name": "portfolio18",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf build/",
    "start": "react-scripts start",
    "prebuild": "npm run clean",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "if git ls-remote gh-pages; then npm run build && push-dir --dir=build --remote=gh-pages --branch=master --cleanup; fi"
  },
  "eslintConfig": {
